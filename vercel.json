{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/(.*)", "has": [{ "type": "host", "value": "your-domain.vercel.app" }], "destination": "/" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}