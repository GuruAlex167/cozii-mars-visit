{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.+\\.[a-z0-9]{1,})$", "dest": "/$1" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
